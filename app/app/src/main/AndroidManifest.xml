<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="alex.ts.app">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".hw_01.TheFirstActivity" />
        <activity android:name=".hw_02.TheSecondActivity" />
        <activity android:name=".hw_03.TheThirdActivity" />
        <activity android:name=".hw_04.TheFourthActivity" />
        <activity android:name=".hw_04.owl_animation.OwlAnimationActivity" />
        <activity android:name=".hw_04.diagram.MyDiagramActivity" />
        <activity android:name=".hw_04.clock.MyClockActivity" />
        <activity android:name=".hw_05.TheFifthActivity" />

        <service android:name=".hw_05.service.WifiChangedService"
            android:enabled="true"/>

    </application>

</manifest>